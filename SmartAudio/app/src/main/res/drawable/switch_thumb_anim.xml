<animated-selector xmlns:android="http://schemas.android.com/apk/res/android"
				   android:constantSize="true">
	<item
			android:state_checked="true"
			android:id="@+id/on">
		<nine-patch
				android:src="@drawable/btn_switch_to_on_frame_12"
				android:gravity="center" />
	</item>
	<item android:id="@+id/off">
		<nine-patch
				android:src="@drawable/btn_switch_to_on_frame_01"
				android:gravity="center" />
	</item>
	<transition
			android:fromId="@+id/off"
			android:toId="@+id/on">
		<animation-list>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_on_frame_01" android:gravity="center" />
			</item>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_on_frame_02" android:gravity="center" />
			</item>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_on_frame_03" android:gravity="center" />
			</item>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_on_frame_04" android:gravity="center" />
			</item>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_on_frame_05" android:gravity="center" />
			</item>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_on_frame_06" android:gravity="center" />
			</item>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_on_frame_07" android:gravity="center" />
			</item>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_on_frame_08" android:gravity="center" />
			</item>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_on_frame_09" android:gravity="center" />
			</item>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_on_frame_10" android:gravity="center" />
			</item>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_on_frame_11" android:gravity="center" />
			</item>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_on_frame_12" android:gravity="center" />
			</item>
		</animation-list>
	</transition>
	<transition android:fromId="@+id/on" android:toId="@+id/off">
		<animation-list>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_off_frame_01" android:gravity="center" />
			</item>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_off_frame_02" android:gravity="center" />
			</item>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_off_frame_03" android:gravity="center" />
			</item>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_off_frame_04" android:gravity="center" />
			</item>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_off_frame_05" android:gravity="center" />
			</item>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_off_frame_06" android:gravity="center" />
			</item>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_off_frame_07" android:gravity="center" />
			</item>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_off_frame_08" android:gravity="center" />
			</item>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_off_frame_09" android:gravity="center" />
			</item>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_off_frame_10" android:gravity="center" />
			</item>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_off_frame_11" android:gravity="center" />
			</item>
			<item android:duration="15">
				<nine-patch android:src="@drawable/btn_switch_to_off_frame_12" android:gravity="center" />
			</item>
		</animation-list>
	</transition>
</animated-selector>
